  <object class="GtkWindow" id="terminal_win">
    <property name="can_focus">False</property>
    <property name="resizable">False</property>
    <property name="deletable">False</property>
    <child>
      <object class="VteTerminal" id="term">
        <property name="visible">True</property>
        <property name="can_focus">True</property>
        <property name="hscroll_policy">natural</property>
        <property name="vscroll_policy">natural</property>
        <property name="encoding">UTF-8</property>
        <property name="scrollback_lines">16</property>
        <property name="scroll_on_keystroke">True</property>
        <property name="scroll_on_output">False</property>
        <signal name="child-exited" handler="on_term_child_exited" swapped="no"/>
        <signal name="contents-changed" handler="on_term_contents_changed" swapped="no"/>
      </object>
    </child>
    <child type="titlebar">
      <placeholder/>
    </child>
  </object>
</interface>
